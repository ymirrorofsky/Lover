<% include header %>
<form class="form-horizontal reg_form" method="post">
  <div class="container">
    <div class="form-group">
      <img src="/images/bgb3.png" alt="logo" class="col-sm-offset-3">
    </div>
    <div class="form-group">
      <div class="col-sm-6 col-sm-offset-4">
        <label> 用户名：</label>
        <input type="text" id="name" class="form-control" name="name" placeholder="请输入文字,最多可五个"><span>√</span>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-6 col-sm-offset-4">
        <label> 密 码： </label>
        <input type="password" id="password" class="form-control" name="password" placeholder="请用6-20位英文或数字组合"><span>√</span>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-6 col-sm-offset-4">
        <label> 确认密码 </label>
        <input type="password" id="passwords" class="form-control" name="password-repeat" placeholder="请和密码保持一致"><span>√</span>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-6  col-sm-offset-4">
        <label> 邮 箱： </label>
        <input type="email" id="email" class="form-control" name="email" placeholder="请输入您的邮箱地址"><span>√</span>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-offset-4 col-sm-6">
        <button type="submit" class="btn btn-success" id="btn" disabled> 立即注册 </button>
      </div>
    </div>
  </div>
</form>




